{
  "apiKey": "your-reachu-api-key-here",
  "environment": "production",
  "theme": {
    "name": "Dark Streaming Theme",
    "mode": "dark",
    "lightColors": {
      "primary": "#007AFF",
      "secondary": "#5856D6",
      "success": "#34C759",
      "warning": "#FF9500",
      "error": "#FF3B30",
      "info": "#64D2FF",
      "background": "#F2F2F7",
      "surface": "#FFFFFF",
      "surfaceSecondary": "#F9F9F9",
      "textPrimary": "#000000",
      "textSecondary": "#8E8E93",
      "textTertiary": "#C7C7CC",
      "border": "#E5E5EA",
      "borderSecondary": "#D1D1D6"
    },
    "darkColors": {
      "primary": "#0066FF",
      "secondary": "#4A90E2",
      "success": "#28CD41",
      "warning": "#FF8C00",
      "error": "#FF3B30",
      "info": "#00A8FF",
      "background": "#000000",
      "surface": "#0D0D0F",
      "surfaceSecondary": "#1A1A1D",
      "textPrimary": "#FFFFFF",
      "textSecondary": "#B0B0B5",
      "textTertiary": "#6D6D70",
      "border": "#2A2A2D",
      "borderSecondary": "#3A3A3D"
    }
  },
  "cart": {
    "floatingCartPosition": "bottomRight",
    "floatingCartDisplayMode": "full",
    "floatingCartSize": "medium",
    "autoSaveCart": true,
    "showCartNotifications": true,
    "enableGuestCheckout": true
  },
  "network": {
    "timeout": 30.0,
    "retryAttempts": 3,
    "enableCaching": true,
    "enableLogging": false
  },
  "ui": {
    "enableAnimations": true,
    "showProductBrands": true,
    "enableHapticFeedback": true
  },
  "liveShow": {
    "tipio": {
      "apiKey": "your-tipio-api-key-here",
      "baseUrl": "https://api.tipio.no",
      "enableWebhooks": true,
      "webhookSecret": "your-webhook-secret"
    },
    "vimeo": {
      "apiKey": "your-vimeo-api-key",
      "accessToken": "your-vimeo-access-token",
      "baseUrl": "https://api.vimeo.com",
      "enableEmbedPlayer": true
    },
    "realTime": {
      "webSocketUrl": "wss://ws.reachu.com",
      "autoReconnect": true,
      "heartbeatInterval": 30,
      "maxReconnectAttempts": 5,
      "componentCacheTimeout": 300,
      "autoRefreshInterval": 60
    },
    "components": {
      "enableDynamicComponents": true,
      "maxConcurrentComponents": 5,
      "defaultAnimationDuration": 0.3,
      "enableOfflineCache": true,
      "preloadNextComponents": true
    },
    "streaming": {
      "autoJoinChat": true,
      "enableAutoplay": false,
      "videoQuality": "auto",
      "enablePictureInPicture": true,
      "enableFullscreen": true,
      "showStreamControls": false,
      "muteByDefault": true
    },
    "chat": {
      "enableChat": true,
      "enableChatModeration": true,
      "maxChatMessageLength": 200,
      "enableEmojis": true,
      "enableChatNotifications": false,
      "chatRefreshInterval": 2.0,
      "enableUserMentions": true,
      "enableChatHistory": true,
      "showChatAvatars": true
    },
    "shopping": {
      "enableShoppingDuringStream": true,
      "showProductOverlays": true,
      "enableQuickBuy": true,
      "productOverlayDuration": 10.0,
      "enableProductNotifications": true,
      "integrateLiveCart": true,
      "specialPricingEnabled": true,
      "countdownEnabled": true
    },
    "ui": {
      "playerAspectRatio": "16:9",
      "enableLiveIndicator": true,
      "showViewerCount": false,
      "enableShareButton": true,
      "layout": {
        "defaultLayout": "fullScreenOverlay",
        "enableLayoutSwitching": true,
        "miniPlayerPosition": "bottomRight"
      },
      "branding": {
        "liveIndicatorColor": "#FF3B30",
        "accentColor": "#0066FF",
        "overlayBackgroundOpacity": 0.90,
        "gradientOverlay": true,
        "cardBackgroundColor": "#0D0D0F",
        "highlightColor": "#00A8FF",
        "shadowColor": "#000000",
        "shadowOpacity": 0.5
      },
      "animations": {
        "enableEntryAnimations": true,
        "enableExitAnimations": true,
        "componentTransitionDuration": 0.4,
        "fadeInDuration": 0.6,
        "slideAnimationEnabled": true
      }
    },
    "notifications": {
      "enableStreamNotifications": true,
      "enableProductNotifications": true,
      "enableComponentNotifications": true,
      "notificationSound": true,
      "showNotificationBadges": true
    }
  }
}

